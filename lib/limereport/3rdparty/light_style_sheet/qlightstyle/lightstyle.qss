{
    color: #000;
    background-color: #f8f8f8;
    border: 1px solid transparent;
}

QTabBar::tab:top:selected:hover {
    background-color: #c3e9a7;
}

QTabBar::tab:top:!selected:hover {
    background-color: #c3e9a7;
}

QTabBar::tab::top:selected
{
    border-top: 2px solid #75bf3f;
    background-color: #fff;

}

/* BOTTOM TABS */
