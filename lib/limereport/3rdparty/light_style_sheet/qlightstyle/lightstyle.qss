{
    color: #000;
    background-color: #f3f3f3;
    border: 1px solid transparent;
}

QTabBar::tab:bottom:selected:hover {
    background-color: #c3e9a7;
}

QTabBar::tab:bottom:!selected:hover {
    background-color: #c3e9a7;
}

QTabBar::tab::bottom:selected
{
    border-bottom: 2px solid #75bf3f;
    background-color: #fff;
}

